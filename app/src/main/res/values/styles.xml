<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextStyle">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/colorWarm_grey</item>
    </style>
    <style name="TextStyle2">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/colorWhite_five</item>
    </style>
    <style name="TextStyle3">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/colorBrown_grey</item>
    </style>
    <style name="TextStyle23">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/colorWarm_grey</item>
    </style>
    <style name="TextStyle24">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">30sp</item>
        <item name="android:lineSpacingExtra">15sp</item>
        <item name="android:textColor">@color/colorBrown_grey</item>
    </style>
    <style name="TextStyle25">
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/colorDark</item>
    </style>
    <style name="TextStyle26">
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">@color/colorBrown_grey</item>
    </style>

    <style name="BrownCheck">
        <item name="colorAccent">#289</item>
    </style>

</resources>
